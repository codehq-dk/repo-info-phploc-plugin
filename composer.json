{
    "name": "codehq-dk/repo-info-phploc-plugin",
    "description": "PhpLoc Repository Information Plugin",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "codehq-dk/repo-info-contracts": "1.0.0-alpha2",
        "codehq-dk/linux-bash-helper": "1.0.0-alpha2"
    },
    "autoload": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\PHPLOC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\PHPLOC\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "lcobucci/clock": "^3.0.0",
        "codehq-dk/repo-info-example-plugin": "1.0.0-alpha2"

    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-report": "vendor/bin/phpunit tests --coverage-html tests/CoverageReport --coverage-filter tests"
    }
}
